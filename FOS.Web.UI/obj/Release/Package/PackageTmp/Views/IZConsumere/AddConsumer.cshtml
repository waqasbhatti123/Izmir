                            $("#CoOwnerName").val(Response.CoOwnerName);
                            $("#CoOwnerCNIC").val(Response.CoOwnerCNIC);
                            $("#Filer").val(Response.Filer);
                            $("#NTN").val(Response.NTN);
                            $("#ConnectionDate").val(Response.ConnectionDate);
                            $("#PTV").val(Response.PTV);
                            $("#MeterNo").val(Response.MeterNo);
                            $("#MeterTypess").val(Response.MeterTypess);
                            $("#ChallanNo").val(Response.ChallanNo);
                            $("#ChallanDate").val(Response.ChallanDate);
                            $("#Month").val(Response.Month);
                            $("#MultiplyingFector").val(Response.MultiplyingFector);
                            $("#CurrentReading").val(Response.CurrentReading);
                            $("#MeterStatusID").val(Response.MeterStatusID);

                            $('html, body').animate({
                                scrollTop: $("#main-content").offset().top
                            }, 500);

                            $("#RegionID").focus();

                        }
                        else { alertify.error('Server Is Not Responding.'); }
                    }
                });
            }
            catch (ex) { }

        });
    }

    if (UpdateCheck == "@HttpContext.Current.Session["DeleteCheck"]") { $(".delete").css("display", "none"); }
