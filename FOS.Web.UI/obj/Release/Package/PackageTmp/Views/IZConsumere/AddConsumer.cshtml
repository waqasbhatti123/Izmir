                                </select>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span12" style="margin-left:0px">
                    <div class="span4">
                        <div class="control-group">
                            <label class="control-label">Challan No:</label>
                            <div class="controls">
                                <input class="text-box single-line" id="ChallanNo" autocomplete="off" name="ChallanNo" type="text" value="" placeholder="Enter Challan No">
                                <span class="field-validation-valid" data-valmsg-for="ChallanNo" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="span4">
                        <div class="control-group">
                            <label class="control-label">Challan Date:</label>
                            <div class="controls">
                                <input class="text-box single-line" id="ChallanDate" autocomplete="off" name="ChallanDate" type="text" value="" placeholder="Enter Challan Date">
                                <span class="field-validation-valid" data-valmsg-for="ChallanDate" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="span4">
                        <div class="control-group">
                            <label class="control-label">Connection Month:</label>
                            <div class="controls">
                                <input class="text-box single-line" id="Month" autocomplete="off" name="Month" type="month" value="" placeholder="Enter ">
                                <span class="field-validation-valid" data-valmsg-for="Month" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span12" style="margin-left:0px">
                    <div class="span4">
                        <div class="control-group">
                            <label class="control-label">M.F:</label>
                            <div class="controls">
                                <input class="text-box single-line" id="MultiplyingFector" autocomplete="off" name="MultiplyingFector" type="text" value="" placeholder="Enter M.F">
                                <span class="field-validation-valid" data-valmsg-for="MultiplyingFector" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="span4">
                        <div class="control-group">
                            <label class="control-label">Current Reading:</label>
                            <div class="controls">
                                <input class="text-box single-line" id="CurrentReading" autocomplete="off" name="CurrentReading" type="text" value="" placeholder="Enter Current Reading">
                                <span class="field-validation-valid" data-valmsg-for="CurrentReading" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="span4">
                        <div class="control-group">
                            <label class="control-label">Meter Status:</label>
                            <div class="controls">
                                @Html.DropDownListFor(model => model.MeterStatusID, new SelectList(Model.MeterStatuses, "MeterStatusID", "StatusName"), new { @class = "" })
                                @Html.ValidationMessageFor(model => model.MeterStatusID)
                            </div>
                        </div>
                    </div>
                </div>
                <div class="span12" style="margin-left:0px;">
                    <div class="control-group" style="border-top: 1px solid #e5e5e5;">
                        <div class="form-actions text-center" style="padding-left:0px;">
                            <button type="submit" id="btnNew" class="btn btn-primary">
                                <i class="icon-ok icon-white" style="padding-right:8px"></i><span id="create">Create</span>
                            </button>
                            <button type="button" id="btnRefresh" class="btn btn-info"><i class="icon-ban-circle icon-white" style="padding-right:8px"></i>Reset</button>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <!-- END SAMPLE FORM PORTLET-->
    </div>
</div>
}
